    "@schematics/angular": "^8.3.17",
    "@types/jasmine": "3.4.1",
    "@types/node": "8.10.54",
    "jasmine": "3.5.0",
    "typescript": "~3.5.3"
  },
  "devDependencies": {
    "@angular-devkit/schematics-cli": "^0.803.17"
